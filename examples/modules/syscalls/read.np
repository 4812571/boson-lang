const sys = mopen("std::syscalls", none)[0]
const no = sys.get_syscalls()

const fd = sys.call(no.OPEN, "hello.txt", 0)

if (fd < 0) {
    println("file hello.txt not found")
    exit(fd)
}

const buffer = create_buffer(10)

sys.call(
    no.READ,
    fd, 
    buffer,
    10
)

println(string(buffer))